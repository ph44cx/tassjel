<div class="jumbotron">
  <div class="row">
    <div class="col-lg-12">
      <div class="well bs-component">
        <%= form_for (@st_tr_file) do |form| %>
          <%= render 'errors', object: @st_tr_file %>
          <fieldset>
            <legend>ST-FILE-TR</legend>
              <div class="col-lg-6">
                <div class="form-group">
                  <label for="st_tr_file_st_ma_un_field" class="control-label">Certificate Source</label>
                  <%= form.text_field :st_ma_un_field, class: 'form-control', id: 'st_tr_file_st_ma_un_field', placeholder: 'Certificate Source', autofocus: true %>
                </div>

                <div class="form-group">
                  <label for="st_tr_file_st_tr_un_source" class="control-label">Certificate Source</label>
                  <%= form.text_field :st_tr_un_source, class: 'form-control', id: 'st_tr_file_st_tr_un_source', placeholder: 'Certificate Source', autofocus: true %>
                </div>

                <div class="form-group">
                  <label for="st_tr_file_st_tr_un_name" class="control-label">Certificate Source</label>
                  <%= form.text_field :st_tr_un_name, class: 'form-control', id: 'st_tr_file_st_tr_un_name', placeholder: 'Certificate Source', autofocus: true %>
                </div>

                <div class="form-group">
                  <label for="st_tr_file_st_tr_un_sem" class="control-label">Certificate Source</label>
                  <%= form.text_field :st_tr_un_sem, class: 'form-control', id: 'st_tr_file_st_tr_un_sem', placeholder: 'Certificate Source', autofocus: true %>
                </div>

                <div class="form-group">
                  <label for="st_tr_file_st_tr_photo1" class="control-label">Certificate Source</label>
                  <%= form.file_field :st_tr_photo1, class: 'form-control', id: 'st_tr_file_st_tr_photo1', placeholder: 'Certificate Source', autofocus: true %>
                </div>
              </div>

              <div class="col-lg-6">

                <div class="form-group">
                  <label for="st_tr_file_st_tr_photo2" class="control-label">Certificate Source</label>
                  <%= form.file_field :st_tr_photo2, class: 'form-control', id: 'st_tr_file_st_tr_photo2', placeholder: 'Certificate Source', autofocus: true %>
                </div>

                <div class="form-group">
                  <label for="st_tr_file_st_tr_extra_photo1" class="control-label">Certificate Source</label>
                  <%= form.file_field :st_tr_extra_photo1, class: 'form-control', id: 'st_tr_file_st_tr_extra_photo1', placeholder: 'Certificate Source', autofocus: true %>
                </div>

                <div class="form-group">
                  <label for="st_tr_file_st_tr_extra_photo2" class="control-label">Certificate Source</label>
                  <%= form.file_field :st_tr_extra_photo2, class: 'form-control', id: 'st_tr_file_st_tr_extra_photo2', placeholder: 'Certificate Source', autofocus: true %>
                </div>

                <div class="form-group">
                  <label for="st_tr_file_st_tr_un_target" class="control-label">Certificate Source</label>
                  <%= form.text_field :st_tr_un_target, class: 'form-control', id: 'st_tr_file_st_tr_un_target', placeholder: 'Certificate Source', autofocus: true %>
                </div>

                <div class="form-group">
                  <label for="st_tr_file_st_tr_un_name" class="control-label">Certificate Source</label>
                  <%= form.text_field :st_tr_un_name, class: 'form-control', id: 'st_tr_file_st_tr_un_name', placeholder: 'Certificate Source', autofocus: true %>
                </div><br/>
                
                <%= form.hidden_field :user_id, value: current_user.id %>

                <div class="actions">
                  <%= form.button 'Save file', :class => "btn btn-primary", style: 'width:65%;', data: {disable_with: "<i class='fa fa-spinner fa-spin'></i>Creating file...."} %>
                </div>

            </div>
            
          </fieldset>
        <% end %>
      </div>
    </div>
  </div>
</div>
